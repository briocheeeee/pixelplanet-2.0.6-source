msgid "Name can't be empty."
msgstr "Το όνομα εν' μπορεί να εν κενό."

#: src/utils/validation.js:30
msgid "Name must be at least 2 characters long"
msgstr "Το όνομα πρέπει να εν πας τους 2 χαρακτήρες."

#: src/utils/validation.js:31
msgid "Name must be shorter than 26 characters"
msgstr "Το όνομα πρέπει να εν πκιο μιτσίν που 26 χαρακτήρες."

#: src/utils/validation.js:38
msgid "Name contains invalid character like @, /, \\ or #"
msgstr "Το όνομα περιέχει άκυρο χαρακτήρα όπως @, /, \\ ή #."

#: src/utils/validation.js:53
msgid "No password given."
msgstr "Εν εδόθηκε κωδικός."

#: src/utils/validation.js:56
msgid "Password must be at least 6 characters long."
msgstr "Ο κωδικός πρέπει να έχει πας τους 6 χαρακτήρες."

#: src/utils/validation.js:59
msgid "Password must be shorter than 60 characters."
msgstr "Ο κωδικός πρέπει να είναι λιότερο που 60 χαρακτήρες."

