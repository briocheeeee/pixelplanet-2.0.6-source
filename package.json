    "passport-reddit": "^1.1.0",
    "passport-vkontakte": "^0.5.0",
    "ppfun-captcha": "^1.7.2",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.3.0",
    "react-redux": "^9.1.2",
