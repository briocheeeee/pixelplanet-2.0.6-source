{
  "name": "pixelplanet",
  "version": "2.0.1",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "type": "module",
  "description": "Unlimited planet canvas for placing pixels",
  "homepage": "https://git.pixelplanet.fun/ppfun/pixelplanet",
  "author": "HF <hf@pixelplanet.fun>",
  "repository": {
    "type": "git",
    "url": "git+https://git.pixelplanet.fun/ppfun/pixelplanet.git"
  },
  "main": "server.js",
  "scripts": {
    "dev": "pm2 kill && npm run build -- --langs en && timeout 3 && cd dist && npm run start",
    "build": "node scripts/build.js",
    "build:server": "node scripts/build.js --server",
    "build:client": "node scripts/build.js --client",
    "build:dev": "node scripts/build.js --langs en --dev",
    "update-browserlist": "browserslist --update-db",
    "restart": "ssh pixelplanet /home/pixelpla/restart.sh",
    "deploy": "./deployment/deploy.sh",
    "deploy:server": "./deployment/deploy-server.sh",
    "minify-css": "node scripts/minifyCss.js",
    "lint": "cd $INIT_CWD && eslint --ext .jsx --ext .js",
    "lint:src": "eslint --ext .jsx --ext .js src"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11"
  ],
  "dependencies": {
    "bcrypt": "^5.1.1",
    "chart.js": "^4.4.4",
